{
  "name": "myevals-docs",
  "version": "1.0.0",
  "private": true,
  "description": "MyEvaluations Interactive Code Documentation System",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3700",
    "dev": "docusaurus start --port 3700",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --port 3700",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "sync-repos": "tsx scripts/sync-repos.ts",
    "parse:dotnet:solution": "tsx scripts/parse-dotnet-solution.ts",
    "parse:dotnet:classes": "tsx scripts/parse-dotnet-classes.ts",
    "parse:dotnet:schedulers": "tsx scripts/parse-dotnet-schedulers.ts",
    "parse:dotnet:sprocs": "tsx scripts/parse-dotnet-sprocs.ts",
    "parse:dotnet:web-files": "tsx scripts/parse-dotnet-web-files.ts",
    "parse:dotnet:all-files": "tsx scripts/parse-dotnet-all-files.ts",
    "parse:dotnet:full": "npm run parse:dotnet:solution && npm run parse:dotnet:classes && npm run parse:dotnet:schedulers && npm run parse:dotnet:sprocs && npm run parse:dotnet:web-files && npm run parse:dotnet:all-files",
    "generate:nodejs": "tsx scripts/generate-nodejs-docs.ts",
    "generate:react": "tsx scripts/generate-react-docs.ts",
    "generate:maui": "tsx scripts/generate-maui-docs.ts",
    "generate:all": "npm run parse:dotnet:solution && npm run parse:dotnet:classes && npm run parse:dotnet:schedulers && npm run parse:dotnet:sprocs && npm run generate:nodejs && npm run generate:react && npm run generate:maui",
    "ai:enrich": "tsx scripts/ai-enrich.ts",
    "ai:enrich:dotnet": "tsx scripts/ai-enrich-dotnet.ts",
    "enrich:merge": "tsx scripts/merge-enrichment-batches.ts",
    "enrich:pages": "tsx scripts/generate-file-reference-pages.ts",
    "enrich:build": "npm run enrich:merge && npm run enrich:pages",
    "generate:reverse-deps": "tsx scripts/generate-reverse-deps.ts && tsx scripts/inject-dependents-panel.ts",
    "prebuild:full": "npm run sync-repos && npm run generate:all",
    "build:full": "npm run prebuild:full && npm run build"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@docusaurus/theme-mermaid": "^3.7.0",
    "@easyops-cn/docusaurus-search-local": "^0.45.0",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/tsconfig": "^3.7.0",
    "@docusaurus/types": "^3.7.0",
    "@octokit/rest": "^21.0.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "d3": "^7.9.0",
    "tree-sitter": "^0.21.1",
    "tree-sitter-c-sharp": "^0.23.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "browserslist": {
    "production": [">0.5%", "not dead", "not op_mini all"],
    "development": ["last 3 chrome version", "last 3 firefox version", "last 5 safari version"]
  }
}
